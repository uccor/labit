                <td>{{ x.nombre }}</td>
                <td>
                    <div class="radio">
                        <label>
                            <input type="radio" name="comp" id="rad2" value="2" checked>
                        </label>
                    </div>
                </td>
            </tr>
            <tr>
                <td>2</td>
                <td> Derecho Penal I </td>
